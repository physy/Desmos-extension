{
  "name": "Desmos Math Font Enhancer",
  "description": "Enhances mathematical expressions in Desmos Calculator with beautiful LaTeX-style fonts.",
  "version": "1.0.7",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    "resource/*.woff2",
    "injected.js"
  ],
  "content_scripts": [
    {
      "css": [
        "content.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "https://www.desmos.com/*"
      ]
    }
  ],
  "permissions": [
    "https://www.desmos.com/*"
  ]
}
